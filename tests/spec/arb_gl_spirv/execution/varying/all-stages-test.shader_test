[geometry shader]
#version 450 core

layout(triangles) in;
layout(triangle_strip, max_vertices = 3) out;

layout(location = 0) in mediump float te_out[];
layout(location = 0) out mediump float geo_out;

void main()
{
	for(int i = 0; i < gl_in.length(); i++) {
		geo_out = te_out[i];

		gl_Position = gl_in[i].gl_Position;
		EmitVertex();
	}
}

