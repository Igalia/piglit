[require]
GLSL >= 1.20

[vertex shader]
void main()
{
	gl_Position = gl_ModelViewProjectionMatrix * gl_Vertex;
}

[fragment shader]
void main()
{
    const float one_half = exp2(16.0) - 65535.5;
    gl_FragColor = vec4(0.0, one_half, 0.0, 1.0);
}

[test]
draw rect -1 -1 2 2
probe all rgb 0.0 0.5 0.0
