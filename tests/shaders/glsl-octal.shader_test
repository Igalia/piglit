[require]
GL >= 2.0
GLSL >= 1.10

[vertex shader]
varying vec4 color;

void main()
{
	ivec4 val = ivec4(0, 01, 010, 04);

	gl_Position = gl_Vertex;
	color = vec4(val) / vec4(1.0, 2.0, 16.0, 8.0);
}

[fragment shader]
varying vec4 color;
void main()
{
	gl_FragColor = color;
}

[test]
draw rect -1 -1 2 2
probe rgba 1 1 0.0 0.5 0.5 0.5
