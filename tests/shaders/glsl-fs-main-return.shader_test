[require]
GL >= 2.0
GLSL >= 1.10

[vertex shader]
void main()
{
	gl_Position = gl_Vertex;
}

[fragment shader]
uniform int early;

void main()
{
	gl_FragColor = vec4(0.0, 1.0, 0.0, 0.0);
	if (early != 0) /* always true */
		return;
	gl_FragColor = vec4(1.0, 0.0, 0.0, 0.0);
}

[test]
uniform int early 1
draw rect -1 -1 2 2
probe all rgba 0.0 1.0 0.0 0.0
