[require]
GLSL >= 1.10
SPIRV NO OTHER (glslang cannot link this)

[vertex shader]
vec4 vals[2];

void set_position();

void main()
{
	vals[1] = gl_Vertex;
	set_position();
}

[vertex shader]
vec4 vals[];

void set_position()
{
	gl_Position = vals[1];
}

[fragment shader]
void main()
{
	gl_FragColor = vec4(0.0, 1.0, 0.0, 0.0);
}

[test]
draw rect -1 -1 2 2
probe all rgba 0.0 1.0 0.0 0.0
