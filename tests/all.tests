def add_fbo_depthstencil_tests(group, format):
	if format == 'default_fb':
		prefix = ''
	else:
		prefix = 'fbo-'
	group[prefix + 'depthstencil-' + format + '-clear'] = PlainExecTest(['fbo-depthstencil', '-auto', 'clear', format])
